{
  "name": "vue-assets",
  "version": "1.0.56",
  "description": "Assets for use in my Vue applications.",
  "type": "module",
  "files": [
    "dist",
    "assets/css",
    "assets/js",
    "assets/data"
  ],
  "main": "./dist/vue-assets.umd.cjs",
  "module": "./dist/vue-assets.js",
  "exports": {
    ".": {
      "import": "./dist/vue-assets.js",
      "require": "./dist/vue-assets.umd.cjs"
    },
    "./assets/css/*": "./dist/assets/css/*",
    "./assets/js/*": "./dist/assets/js/*",
    "./assets/data/*": "./dist/assets/data/*"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "uuid": "^10.0.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-select": "^4.0.0-beta.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "^1.77.5",
    "vite": "^5.1.6"
  }
}
